    "# 팩토리얼 출력 문제 \n",
    "\n",
    "def factorial(N):\n",
    "  if (N>1) & (N<=12):\n",
    "    return N*factorial(N-1)\n",
    "  else:\n",
    "    return 1\n",
    "\n",
    "  \n",
    "\n",
    "a = 5 # a = int(input()) \n",
    "\n",
    "print(factorial(a))\n"
   ]
