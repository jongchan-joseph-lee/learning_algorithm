    "# 피보나치수열 출력 문제\n",
    "def fibo(x):\n",
    "\n",
    "  if (x==1):\n",
    "    return 1\n",
    "\n",
    "  if (x==0):\n",
    "    return 0\n",
    "  \n",
    "  if (x>=2):\n",
    "    return fibo(x-1) + fibo (x-2)\n",
    "\n",
    "\n",
    "\n",
    "a = 10 # a = int(input())\n",
    "\n",
    "print(fibo(a))\n"
   ]
  }
