# 별 찍기 - 10 문제

def star(x):
  div = x//3
  if (x==3):
    f[1][:3] = ['*',' ','*']
    f[0][:3] = ['*']*3
    f[2][:3] = ['*']*3
    return

  star(div)

  for i in range(0, x, div):
    for j in range(0, x, div):
      if i != div or j != div:
        for k in range(div):
          f[i+k][j:j+div] = f[k][:div]

n = int(input())
f = [[' ' for _ in range(n)] for _ in range(n)]

star(n)

for i in range(n):
  for j in range(n):
    print(f[i][j],end='')
  print()
